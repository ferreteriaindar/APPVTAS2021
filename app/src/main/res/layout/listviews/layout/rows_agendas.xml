<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <TextView
        android:id="@+id/txtLayoutAgendasNombretextView4"
        android:layout_width="310dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/txtLayoutAgendasCte"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtLayoutAgendasNombretextView4"
        android:layout_alignParentStart="true"
        android:text="TextView"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="65dp"
        android:layout_height="57dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/txtLayoutAgendasDir"
        android:layout_width="308dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtLayoutAgendasCte"
        android:layout_alignEnd="@+id/txtLayoutAgendasCte"
        android:layout_marginEnd="-250dp"
        android:text="TextView" />
</RelativeLayout>