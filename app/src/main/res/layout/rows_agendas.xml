<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="#eeeeee"
    >

    <TextView
        android:id="@+id/txtLayoutAgendasNombretextView4"
        android:layout_width="310dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="25dp"
        android:textSize="14sp"
        android:maxLines="1"/>

    <TextView
        android:id="@+id/txtLayoutAgendasCte"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtLayoutAgendasNombretextView4"
        android:layout_alignParentStart="true"
        android:layout_marginStart="7dp"
        android:layout_marginTop="-41dp"
        android:text="TextView"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imgAgendasIMG"
        android:layout_width="33dp"
        android:layout_height="30dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="11dp" />

    <TextView
        android:id="@+id/txtLayoutAgendasDir"
        android:layout_width="311dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtLayoutAgendasNombretextView4"
        android:layout_alignEnd="@+id/txtLayoutAgendasCte"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="-244dp"
        android:text="TextView" />
</RelativeLayout>